{
  "name": "auto_express_project_ts",
  "version": "1.0.0",
  "description": "通用express项目框架ts版本",
  "main": "index.js",
  "scripts": {
    "dev": " nodemon",
    "start": "npm run build &&NODE_ENV=production node  build/index.js",
