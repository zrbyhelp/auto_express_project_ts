    "@sentry/profiling-node": "^1.3.5",
    "@types/dotenv": "^8.2.0",
    "@types/express-winston": "^4.0.0",
    "@types/js-yaml": "^4.0.8",
    "@types/sequelize": "^4.28.20",
    "@types/winston": "^2.4.4",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "js-yaml": "^4.1.0",
    "mysql2": "^3.8.0",
    "sequelize": "^6.35.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
